{
    "name": "talp1/spin8",
    "description": "The Spin8 framework. A framework to help developers to create Wordpress plugins.",
    "keywords": [
        "framework",
        "wordpress",
        "wordpress-plugin",
        "spin8"
    ],
    "authors": [
        {
            "name": "Talp1",
            "homepage": "https://github.com/Talpx1"
        }
    ],
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Spin8\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spin8\\Tests\\": "tests/",
            "WP_Mock\\": "tmp_packages/wp_mock/php/WP_Mock"
        },
        "classmap": [
            "tmp_packages/wp_mock/php/WP_Mock.php"
        ]
    },
    "require": {
        "php": ">=8.2",
        "latte/latte": "^3.0",
        "psr/container": "^2.0",
        "thecodingmachine/safe": "^2.5"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "mikey179/vfsstream": "^1.6",
        "mockery/mockery": "^1.6",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.2",
        "rector/rector": "^0.17.5",
        "slevomat/coding-standard": "^8.13",
        "squizlabs/php_codesniffer": "^3.7",
        "symfony/var-dumper": "^6.3",
        "szepeviktor/phpstan-wordpress": "^1.3",
        "thecodingmachine/phpstan-safe-rule": "^1.2"
    },
    "replace": {
        "10up/wp_mock": "self.version"
    },
    "provide": {
        "psr/container-implementation": "1.1|2.0"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}